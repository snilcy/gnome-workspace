{
  "compilerOptions": {
    "noEmit": false,
    "declaration": false,
    "target": "ES2020",
    "module": "None",
    "noEmitOnError": true,
    "strict": true,
    "noFallthroughCasesInSwitch": true,
    "esModuleInterop": false,
    "forceConsistentCasingInFileNames": false,
    "skipLibCheck": true,
    "removeComments": true,
    "incremental": true
  },
  "include": ["./src/api.ts", "./src/extension.ts"],
  "exclude": []
}
